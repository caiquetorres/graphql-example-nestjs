{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@graphql-example/google-auth/(.*)": "<rootDir>/../libs/google-auth/src/$1",
    "@graphql-example/google-auth": "<rootDir>/../libs/google-auth/src",
    "@graphql-example/facebook-auth/(.*)": "<rootDir>/../libs/facebook-auth/src/$1",
    "@graphql-example/facebook-auth": "<rootDir>/../libs/facebook-auth/src"
  }
}