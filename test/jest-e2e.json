{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@graphql-example/google-sign-in/(.*)": "<rootDir>/../libs/google-sign-in/src/$1",
    "@graphql-example/google-sign-in": "<rootDir>/../libs/google-sign-in/src"
  }
}